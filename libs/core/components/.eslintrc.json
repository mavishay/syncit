{
  "extends": [
    "plugin:@nrwl/nx/react",
    "../../../.eslintrc.json",
    "airbnb",
    "airbnb-typescript"
  ],
  "ignorePatterns": ["!**/*"],
  "overrides": [
    {
      "files": ["*.ts", "*.tsx", "*.js", "*.jsx"],
      "rules": {
        "react/react-in-jsx-scope": 0,
        "import/no-named-as-default": 0
      }
    },
    {
      "files": ["*.ts", "*.tsx"],
      "rules": {}
    },
    {
      "files": ["*.js", "*.jsx"],
      "rules": {}
    }
  ]
}
